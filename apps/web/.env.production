# MST-AI Platform Frontend Production Environment

# API配置 - 通过nginx代理
VITE_API_BASE_URL=
VITE_API_TIMEOUT=30000

# WebSocket配置 - 使用相对路径
VITE_WS_URL=

# 文件上传配置
VITE_MAX_FILE_SIZE=524288000  # 500MB
VITE_ALLOWED_FILE_TYPES=.pdf,.doc,.docx,.xls,.xlsx,.ppt,.pptx,.dwg,.dxf,.ifc,.rvt,.skp,.3dm

# AI服务配置
